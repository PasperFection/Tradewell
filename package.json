{
  "name": "tradewell",
  "version": "1.3.8",
  "description": "Een veilige en gebruiksvriendelijke browser-extensie voor geautomatiseerd handelen op het Bitvavo platform",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "build:ts": "tsc",
    "build:bundle": "webpack --config webpack.config.js",
    "build": "npm run clean && npm run build:ts && npm run build:bundle",
    "watch": "webpack --config webpack.config.js --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write 'src/**/*.{ts,js,json,css,md}'",
    "dev": "npm run watch",
    "build:prod": "NODE_ENV=production npm run build && npm run zip:extension",
    "db:migrate": "cd supabase/migrations && psql -h $DB_HOST -U postgres -d postgres -f 20231214_create_credentials_table.sql",
    "db:reset": "cd supabase/migrations && psql -h $DB_HOST -U postgres -d postgres -c 'DROP TABLE IF EXISTS credentials CASCADE;'",
    "zip:extension": "cd dist && zip -r ../tradewell-v1.3.8.zip .",
    "release": "npm run test && npm run lint && npm run build:prod"
  },
  "keywords": [
    "trading",
    "bitvavo",
    "cryptocurrency",
    "browser-extension",
    "typescript"
  ],
  "author": "PasperFection",
  "repository": {
    "type": "git",
    "url": "https://github.com/PasperFection/tradewell.git"
  },
  "bugs": {
    "url": "https://github.com/PasperFection/tradewell/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/chrome": "^0.0.254",
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.17.10",
    "@types/webextension-polyfill": "^0.12.1",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.13.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^25.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^3.1.1",
    "style-loader": "^3.3.3",
    "supabase": "^2.0.0",
    "terser-webpack-plugin": "^5.3.11",
    "ts-jest": "^29.2.5",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.18.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.7",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "dompurify": "^3.2.3",
    "dotenv": "^16.4.7",
    "technicalindicators": "^3.1.0",
    "trading-signals": "^3.7.0",
    "webextension-polyfill": "^0.10.0"
  }
}
